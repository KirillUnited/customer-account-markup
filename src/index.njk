---
layout: base.html
title: Customer Account
---

<div class="section">
    <div class="container">
        <div class="section-inner">
            <div class="section-content">
                <h1>{{ title }}</h1>
                <ul>
                    <li>
                        <a href="https://aesthetic-lebkuchen-e8538c.netlify.app/login/signup">Sign Up</a>
                    </li>
                    <li>
                        <a href="https://aesthetic-lebkuchen-e8538c.netlify.app/login/signin">Sign In</a>
                    </li>
                    <li>
                        <a href="{{ account.path }}">{{ account.title }}</a>
                        <ul>
                            {% for action in account.actions %}
                                <li>
                                    <a href="{{ action.link }}">{{ action.name }}</a>
                                </li>
                            {% endfor %}
                            <li>
                                <a href="/pages/account/payments-holder">payments-holder</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a href="{{ tickets.path }}">{{ tickets.title }}</a>
                    </li>
                    <li>
                        <a href="/pages/registration">Edit Registration Information</a>
                    </li>
                    <li>
                        <a href="{{ ticketdetails.path }}">Ticket/Booking Details</a>
                        <ul>
                            {% for action in ticketdetails.actions %}
                                <li>
                                    <a href="{{ action.link }}">{{ action.title }}</a>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>
                    <li>
                        <a href="{{ events.path }}">{{ events.title }}</a>
                    </li>
                    <li>
                        <a href="{{ commission.path }}">{{ commission.title }}</a>
                        <ul>
                            {% for key, value in commission.actions %}
                                <li>
                                    <a href="{{ value.link }}">{{ value.title }}</a>
                                </li>
                            {% endfor %}
                            <li>
                                <a href="{{ commission.actions.networks.details.path }}">{{ commission.actions.networks.details.title }}</a>
                            </li>
                            <li>
                                <a href="{{ commission.actions.networks.placeholder.link }}">{{ commission.actions.networks.title }}
                                    - empty</a>
                            </li>
                            <li>
                                <a href="{{ commission.actions.referrals.placeholder.link }}">{{ commission.actions.referrals.title }}
                                    - empty</a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>
